{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":preserveSemverRanges"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [ "minor", "patch", "pin", "digest" ],
      "automerge": true,
      "automergeStrategy": "squash"
    }
  ],
  "rangeStrategy": "auto"
}
